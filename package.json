{
  "name": "angular2-webpack-starter",
  "version": "1.0.0",
  "description": "angular2-webpack-starter",
  "main": "index.js",
  "scripts": {
    "ngc": "rimraf dist/ngc-compiled && ngc -p tsconfig.aot.json",
    "start": "webpack-dev-server --profile --display-error-details --inline --progress --port 8080",
    "build-jit": "rimraf dist/jit && webpack --config config/webpack.jit.js --progress --profile --bail",
    "build-aot": "npm run ngc && rimraf dist/aot && webpack --config config/webpack.aot.js --progress --profile --bail",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lon-yang/angular2-webpack-starter.git"
  },
  "keywords": [],
  "author": "Lon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lon-yang/angular2-webpack-starter/issues"
  },
  "homepage": "https://github.com/lon-yang/angular2-webpack-starter#readme",
  "dependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/compiler-cli": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/forms": "^2.1.0",
    "@angular/http": "^2.1.0",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@angular/router": "^3.1.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-rc.1",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.5.2",
    "null-loader": "^0.1.1",
    "postcss": "^5.2.4",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.9.0"
  }
}